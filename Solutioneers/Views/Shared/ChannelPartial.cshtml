@model IEnumerable<Solutioneers.Models.Channel>

@Html.ActionLink("Create a new Channel", "../channels/create");
<div class="row">
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Title)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Votes)
            </th>
            <th></th>
        </tr>
               @foreach (var item in Model)
                {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Title)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Description)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Votes.Count)
                    </td>
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.ChannelID }) |
                        @Html.ActionLink("Details", "Details", new { id = item.ChannelID }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.ChannelID })
                    </td>
                </tr>
            
        }

    </table>
</div>